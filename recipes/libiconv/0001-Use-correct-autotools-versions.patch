Subject: [PATCH 1/3] Use correct autotools versions

---
 Makefile.devel            | 8 ++++----
 libcharset/Makefile.devel | 6 +++---
 preload/Makefile.devel    | 4 ++--
 3 files changed, 9 insertions(+), 9 deletions(-)

diff --git a/Makefile.devel b/Makefile.devel
index 8f6399c..2c1ce59 100644
--- a/Makefile.devel
+++ b/Makefile.devel
@@ -4,10 +4,10 @@
 
 SHELL = /bin/sh
 MAKE = make
-AUTOCONF = autoconf-2.68
-AUTOHEADER = autoheader-2.68
-AUTOMAKE = automake-1.11
-ACLOCAL = aclocal-1.11
+AUTOCONF = autoconf
+AUTOHEADER = autoheader
+AUTOMAKE = automake
+ACLOCAL = aclocal
 GPERF = gperf
 CC = gcc
 CFLAGS = -O
diff --git a/libcharset/Makefile.devel b/libcharset/Makefile.devel
index 174cd15..9a6fa60 100644
--- a/libcharset/Makefile.devel
+++ b/libcharset/Makefile.devel
@@ -3,9 +3,9 @@
 
 SHELL = /bin/sh
 MAKE = make
-AUTOCONF = autoconf-2.68
-AUTOHEADER = autoheader-2.68
-ACLOCAL = aclocal-1.11
+AUTOCONF = autoconf
+AUTOHEADER = autoheader
+ACLOCAL = aclocal
 CP = cp
 RM = rm -f
 
diff --git a/preload/Makefile.devel b/preload/Makefile.devel
index 544059a..c8ac0a8 100644
--- a/preload/Makefile.devel
+++ b/preload/Makefile.devel
@@ -3,8 +3,8 @@
 
 SHELL = /bin/sh
 MAKE = make
-AUTOCONF = autoconf-2.68
-ACLOCAL = aclocal-1.11
+AUTOCONF = autoconf
